{
  "name": "raul-gym-fitness",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "ngrok-dev": "run-p dev-next ngrok",
    "devp": "pkill -f 'next dev' && npm run dev",
    "predev": "dotenv -e .env.development -- tsx scripts/check-env.ts",
    "dev": "dotenv -e .env.development -- next dev --hostname 0.0.0.0 --port 3000",
    "ngrok": "ngrok http 3000",
    "prod": "dotenv -e .env.production -- next build && next start",
    "prebuild": "dotenv -e .env.production -- tsx scripts/check-env.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "type": "tsc --noEmit",
    "test": "playwright test",
    "pw": "playwright test",
    "test:ai": "AI_TEST=true playwright test --max-failures=1",
    "test:fix": "playwright test --max-failures=1 --workers=1",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "postinstall": "prisma generate",
    "prisma": "npm run prisma-schema && npm run prisma-types && npm run prisma-seed ",
    "prismaa": "npm run prisma-schema && npm run prisma-types && npm run prisma-seed && npm run prisma-studio",
    "prisma-schema": "dotenv -e .env.development -- npx prisma db push",
    "prisma-types": "dotenv -e .env.development -- npx prisma generate",
    "prisma-seed": "dotenv -e .env.development -- tsx prisma/seed.ts",
    "prisma-studio": "dotenv -e .env.development -- npx prisma studio",
    "branch": "git branch --show-current",
    "prisma-schema:prod": "dotenv -e .env.production -- npx prisma db push",
    "prisma-types:prod": "dotenv -e .env.production -- npx prisma generate",
    "prisma-seed:prod": "dotenv -e .env.production -- tsx prisma/seed.ts",
    "prisma:prod": "npm run prisma-schema:prod && npm run prisma-types:prod && npm run prisma-seed:prod",
    "prisma:check-prod": "dotenv -e .env.production -- tsx scripts/check-env.ts",
    "prereset:prod": "npm run prisma:check-prod",
    "prisma-reset:prod": "dotenv -e .env.production -- npx prisma db push --force-reset --accept-data-loss",
    "prisma:reset-prod": "npm run prereset:prod && npm run prisma-reset:prod && npm run prisma-schema:prod && npm run prisma-types:prod && npm run prisma-seed:prod",
    "prisma-studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:psql": "docker-compose exec postgres psql -U postgres -d raul_gym_fitness",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "dev:docker": "npm run docker:up && npm run dev"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "big.js": "^7.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fastest-levenshtein": "^1.0.16",
    "licia": "^1.48.0",
    "lucide-react": "^0.539.0",
    "motion": "^12.23.22",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.0",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "use-debounce": "^10.0.6",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@types/big.js": "^6.2.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.14.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
